{
  "name": "iwe",
  "displayName": "IWE",
  "description": "Makdown notes navigation, auto-complete, search, etc",
  "version": "0.0.2",
  "license": "MIT",
  "icon": "assets/logo.png",
  "publisher": "IWE",
  "repository": "https://github.com/iwe-org/iwe",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "keywords": [
    "markdown",
    "pkm",
    "lsp",
    "note",
    "notes",
    "link",
    "links",
    "references",
    "zettelkasten"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iwe.editor",
        "title": "IWE"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@types/tar": "^6.1.11"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0",
    "which": "^3.0.1",
    "tar": "^6.2.0"
  }
}
